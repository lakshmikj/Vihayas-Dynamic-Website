<section style="margin-top: 88px;">
    <div class="container fadeInDown third">
        <table class="table mt-5" id="allUser">
            <thead class="tableHead">
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody style="text-align: center;">
                {{#each users}}
                <tr>
                    <td>{{this.Name}}</td>

                    <td>{{this.Email}}</td>




                </tr>
                {{/each}}
            </tbody>
        </table>

    </div>
</section>
<script>
    $(function () {
        $('#allUser').DataTable();
    })
</script>